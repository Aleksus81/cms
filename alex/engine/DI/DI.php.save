namespace Engine\DI
  class DI 
{
  /**
   * @var array
   */
private $container = [];
  
 /**
  * @param $key
  * @param $value
  * @param $this
  */
  public function set($key, $value)
  {
  $this->container[$key] = $value;
    
    return $this;
  }
  
  /**
  * @param $key
  * @return mixed
  */ 
  
  public function get($key)
      {
  return $this->has[$key];
    
   /**
  * @param $key
  * @return bool
  */ 
  }
  public  function has($key)
  {
  return isset($this->container[$key]);
  }
}

